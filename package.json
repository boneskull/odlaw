{
  "name": "odlaw",
  "version": "0.0.0",
  "description": "CLI config and arg parser for TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/odlaw"
  },
  "homepage": "https://github.com/boneskull/odlaw",
  "bugs": {
    "url": "https://github.com/boneskull/odlaw/issues"
  },
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "license": "Apache-2.0",
  "engines": {
    "node": "^16.20.0 || ^18.0.0 || ^20.0.0",
    "npm": ">=7"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [],
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "dev": "tsc -b --watch",
    "dev:docs": "typedoc --watch",
    "husky-install": "husky install",
    "lint": "run-p lint:eslint lint:md",
    "lint:commit": "commitlint",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:md": "markdownlint-cli2 \"README.md\"",
    "lint:staged": "lint-staged",
    "prepare": "run-p husky-install rebuild",
    "pretest:e2e": "run-s build",
    "rebuild": "run-s clean build",
    "test": "run-p test:e2e test:types",
    "test:ci": "run-s test:e2e test:types",
    "test:e2e": "mocha --timeout 20s --slow 10s \"test/e2e/**/*.spec.ts\"",
    "test:types": "tsd",
    "test:unit": "mocha \"test/unit/**/*.spec.ts\""
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "dependencies": {
    "lilconfig": "2.1.0",
    "type-fest": "^4.3.1",
    "yargs": "17.7.2",
    "zod-validation-error": "1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@tsconfig/node16": "16.1.1",
    "@types/debug": "4.1.8",
    "@types/is-file-esm": "1.0.0",
    "@types/mocha": "10.0.1",
    "@types/node": "18.17.12",
    "@types/pluralize": "0.0.30",
    "@types/sinon": "10.0.16",
    "@types/source-map-support": "0.5.7",
    "@types/which": "3.0.0",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-semistandard": "17.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "markdownlint-cli2": "0.9.2",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "prettier-plugin-pkg": "0.18.0",
    "rewiremock": "3.14.5",
    "sinon": "15.2.0",
    "strip-ansi": "5.2.0",
    "ts-node": "10.9.1",
    "tsd": "0.29.0",
    "typedoc": "0.25.1",
    "typedoc-plugin-zod": "1.1.0",
    "typescript": "5.2.2",
    "unexpected": "13.2.1",
    "unexpected-eventemitter": "2.4.0",
    "unexpected-sinon": "11.1.0",
    "zod": "3.22.2"
  },
  "overrides": {
    "debug": "4.3.4"
  },
  "prettier": {
    "bracketSpacing": false,
    "endOfLine": "auto",
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-pkg"
    ]
  }
}
